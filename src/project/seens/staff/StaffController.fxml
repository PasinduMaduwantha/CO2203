<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.frames.staff.StaffController">
   <children>
      <Label layoutX="254.0" prefHeight="82.0" prefWidth="90.0" text="Staff" textFill="#383a4a">
         <font>
            <Font name="Comic Sans MS" size="30.0" />
         </font>
      </Label>
      <Button fx:id="newItem" layoutX="58.0" layoutY="81.0" mnemonicParsing="false" onAction="#addStaffOnAction" prefHeight="59.0" prefWidth="193.0" text="Add Staff" textFill="#c21f35">
         <font>
            <Font name="Comic Sans MS" size="20.0" />
         </font>
      </Button>
      <Button fx:id="newCategory" layoutX="368.0" layoutY="81.0" mnemonicParsing="false" onAction="#editOnAction" prefHeight="59.0" prefWidth="193.0" text="Edit Staff" textFill="#b51111">
         <font>
            <Font name="Comic Sans MS" size="20.0" />
         </font>
      </Button>
      <Button fx:id="showItems" layoutX="58.0" layoutY="170.0" mnemonicParsing="false" onAction="#showStaffOnAction" prefHeight="59.0" prefWidth="193.0" text="Show Staff" textFill="#bf1717">
         <font>
            <Font name="Comic Sans MS" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
